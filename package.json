    "scripts": {
        "lint": "eslint .",
        "watch": "NODE_ENV=dev run-p \"build:chrome --watch\" \"build:firefox --watch\"",
        "watch:firefox": "NODE_ENV=dev run-s \"build:firefox --watch\"",
        "dev": "NODE_ENV=dev run-p build:*",
        "beta": "NODE_ENV=beta run-p build:*",
        "release": "NODE_ENV=release run-p build:*",
        "build:chrome": "BROWSER=chrome webpack --config tasks/chrome/webpack.chrome.js",
        "build:firefox": "BROWSER=firefox webpack --config tasks/firefox/webpack.firefox.js",
        "build:edge": "BROWSER=edge webpack --config tasks/edge/webpack.edge.js",
        "locales:download": "LOCALES=DOWNLOAD node tasks/locales.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "locales:upload": "LOCALES=UPLOAD node tasks/locales.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn test"
        }
    },
